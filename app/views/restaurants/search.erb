<h2>Search result</h2>
<div class="owl-carousel">
<div class="row">
     <% @restaurants.each do |restaurant|%>
  <div class="col-md-3">
		   <h3><%= restaurant.name%></h3>
       <a href="/restaurants/<%= restaurant.id %>">click to review
       <%= image_tag restaurant.rest_img.url(:rest_index), class:"rest" %>
    </a>

     </div>
       <% end %>

  </div>
<%= paginate @restaurants %>
